{
  "name": "translation-hub",
  "version": "1.0.0",
  "description": "Translation Hub - Intern Translation Management and Evaluation Portal",
  "main": "index.js",  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "cd backend && npm start",
    "seed": "cd backend && npm run seed",
    "check-mongo": "cd backend && npm run check-mongo",
    "test-api": "cd backend && npm run test-api",
    "setup": "npm run install:all && npm run check-mongo && npm run seed"
  },
  "keywords": [
    "translation",
    "management",
    "evaluation",
    "portal",
    "intern"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
